	<Target Name="BeforeBuild">
		<ItemGroup>
			<SATDrivers Include="$(PKG_UCLIENT_TESTEXTENSIONS)\**\*" />
			<AccessibilityJScripts Include="$(PKG_UCLIENT)\testcases\NonShip\QA\TestCases\AccessibilityJScripts\*.*" />
			<JsonConfig Include="$(PKG_UCLIENT)\testcases\NonShip\QA\TestCases\JsonConfig\*.*" />
		</ItemGroup>	
		<MakeDir Directories="$(TargetDir)\SATDrivers" Condition="!Exists('$(TargetDir)\SATDrivers')" />
		<Copy SourceFiles="@(SATDrivers)" DestinationFolder="$(TargetDir)\SATDrivers" Retries="20" SkipUnchangedFiles="true" RetryDelayMilliseconds="1000" ContinueOnError="true" />
		
		<MakeDir Directories="$(TargetDir)\AccessibilityJScripts" Condition="!Exists('$(TargetDir)\AccessibilityJScripts')" />
		<Copy SourceFiles="@(AccessibilityJScripts)" DestinationFolder="$(TargetDir)\AccessibilityJScripts" Retries="20" SkipUnchangedFiles="true" RetryDelayMilliseconds="1000" ContinueOnError="true" />
		
		<MakeDir Directories="$(TargetDir)\JsonConfig" Condition="!Exists('$(TargetDir)\JsonConfig')" />
		<Copy SourceFiles="@(JsonConfig)" DestinationFolder="$(TargetDir)\JsonConfig" Retries="20" SkipUnchangedFiles="true" RetryDelayMilliseconds="1000" ContinueOnError="true" />
	</Target>
	<Target Name="AfterBuild">
		<ItemGroup>
			<SourceFiles Include="$(TargetDir)\*.*" />
		</ItemGroup>
		<!-- execute only when DropInConfigurationFolder is true -->
		<Copy SourceFiles="@(SourceFiles)" DestinationFolder="$(WSRoot)\drop\$(BuildConfiguration)\$(BuildPlatform)\Tests" Retries="20" SkipUnchangedFiles="true" RetryDelayMilliseconds="1000" ContinueOnError="true" Condition="('$(DropInConfigurationFolder)' == 'true')" />
		<Copy SourceFiles="@(SATDrivers)" DestinationFolder="$(WSRoot)\drop\$(BuildConfiguration)\$(BuildPlatform)\Tests\SATDrivers\%(RecursiveDir)" Retries="20" SkipUnchangedFiles="true" RetryDelayMilliseconds="1000" ContinueOnError="true" Condition="('$(DropInConfigurationFolder)' == 'true')" />
		<Copy SourceFiles="@(AccessibilityJScripts)" DestinationFolder="$(WSRoot)\drop\$(BuildConfiguration)\$(BuildPlatform)\Tests\AccessibilityJScripts\%(RecursiveDir)" Retries="20" SkipUnchangedFiles="true" RetryDelayMilliseconds="1000" ContinueOnError="true" Condition="('$(DropInConfigurationFolder)' == 'true')" />
		<Copy SourceFiles="@(JsonConfig)" DestinationFolder="$(WSRoot)\drop\$(BuildConfiguration)\$(BuildPlatform)\Tests\JsonConfig\%(RecursiveDir)" Retries="20" SkipUnchangedFiles="true" RetryDelayMilliseconds="1000" ContinueOnError="true" Condition="('$(DropInConfigurationFolder)' == 'true')" />
			
		<!-- execute only when DropInConfigurationFolder is not true -->
		<Copy SourceFiles="@(SourceFiles)" DestinationFolder="$(WSRoot)\drop\Tests" Retries="20" SkipUnchangedFiles="true" RetryDelayMilliseconds="1000" ContinueOnError="true" Condition="('$(DropInConfigurationFolder)' == 'false') OR ('$(DropInConfigurationFolder)' == '')" />
		<Copy SourceFiles="@(SATDrivers)" DestinationFolder="$(WSRoot)\drop\Tests\SATDrivers\%(RecursiveDir)" Retries="20" SkipUnchangedFiles="true" RetryDelayMilliseconds="1000" ContinueOnError="true" Condition="('$(DropInConfigurationFolder)' == 'false') OR ('$(DropInConfigurationFolder)' == '')" />
		<Copy SourceFiles="@(AccessibilityJScripts)" DestinationFolder="$(WSRoot)\drop\Tests\AccessibilityJScripts\%(RecursiveDir)" Retries="20" SkipUnchangedFiles="true" RetryDelayMilliseconds="1000" ContinueOnError="true" Condition="('$(DropInConfigurationFolder)' == 'false') OR ('$(DropInConfigurationFolder)' == '')" />
		<Copy SourceFiles="@(JsonConfig)" DestinationFolder="$(WSRoot)\drop\Tests\JsonConfig\%(RecursiveDir)" Retries="20" SkipUnchangedFiles="true" RetryDelayMilliseconds="1000" ContinueOnError="true" Condition="('$(DropInConfigurationFolder)' == 'false') OR ('$(DropInConfigurationFolder)' == '')" />
	</Target>
</Project>